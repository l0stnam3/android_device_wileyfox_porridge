        <item>9000</item>
        <item>11000</item>
        <item>13000</item>
        <item>15000</item>
        <item>17000</item>
    </integer-array>

    <integer-array name="config_autoBrightnessLcdBacklightValues">
        <item>1</item>
        <item>12</item>
        <item>91</item>
        <item>103</item>
        <item>107</item>
        <item>113</item>
        <item>120</item>
